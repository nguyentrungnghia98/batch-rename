<Window x:Class="Batch_rename.NamesakeDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Batch_rename"
        mc:Ignorable="d"
        Title="Warning Namesake" Height="316.739" Width="576.861">
    <Grid Margin="0,0,2,-3">
        <Button x:Name="btnCancel" Content="Cancel" HorizontalAlignment="Center" Margin="241,236,238,0" VerticalAlignment="Top" Width="88" BorderBrush="{x:Null}" Height="26" Background="WhiteSmoke" Click="BtnCancel_Click"/>
        <Label x:Name="label" Content="Some files have the same name" HorizontalAlignment="Center" Margin="116,36,82,0" VerticalAlignment="Top" Height="63" FontSize="24" Width="369"/>
        <Button x:Name="btnAddSuffixes" Content="Add the suffixes in the end." HorizontalAlignment="Center" Margin="170,110,166,0" VerticalAlignment="Top" Width="231" Height="50" BorderBrush="{x:Null}" Background="#FFF1F1F1" FontSize="16" Click="BtnAddSuffixes_Click"/>
        <Button x:Name="btnSkipDuplicate" Content="Skip duplicate files." HorizontalAlignment="Center" Margin="170,177,166,0" VerticalAlignment="Top" Width="231" Height="46" BorderBrush="{x:Null}" Background="#FFF1F1F1" FontSize="16" Click="BtnSkipDuplicate_Click"/>

    </Grid>
</Window>
